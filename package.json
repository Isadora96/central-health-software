    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.199",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.9",
